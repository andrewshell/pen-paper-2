{% extends "layout.html.twig" %}

{% block content %}
<ul class="list-inline">
{% for letter in 'A' .. 'Z' %}
    <li><a href="/game-lines/{{ letter }}/">{{ letter }}</a></li>
{% endfor %}
</ul>

{% if game_lines | length > 0 %}
    <ul>
    {% for game_line in game_lines %}
        <li><a href="/game-line/{{ game_line.game_line_id }}/">
            {{ game_line.game_line }}
        </a></li>
    {% endfor %}
    </ul>
{% else %}
    <p>No Publishers Found</p>
{% endif %}
{% endblock content %}
