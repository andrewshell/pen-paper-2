{% if not entity.magazine_article_creators is empty %}
<table class="table">
<tr>
    <th>Article</th>
    <th>Credit</th>
</tr>
{% for magazine_article_creator in entity.magazine_article_creators %}
<tr>
    <th><a href="/magazine-article/{{ magazine_article_creator.magazine_article.magazine_article_id }}/">
        &quot;{{ magazine_article_creator.magazine_article.title }}&quot;
    </a> in <a href="/magazine/{{ magazine_article_creator.magazine_article.magazine.magazine_id }}/">
        {{ magazine_article_creator.magazine_article.magazine.magazine_title.title }} #{{ magazine_article_creator.magazine_article.magazine.issue_number}}
    </a> {% if magazine_article_creator.magazine_article.magazine.cover_date is not empty %}({{ magazine_article_creator.magazine_article.magazine.cover_date }}){% endif %}</th>
    <th>{{ magazine_article_creator.credit.credit }}</th>
</tr>
{% endfor %}
</table>
{% endif %}
